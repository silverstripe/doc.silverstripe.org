{
  "name": "doc.silverstripe.org",
  "version": "1.0.0",
  "description": "SilverStripe CMS Documentation - Next.js Rebuild",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --port 9876",
    "mock": "NEXT_USE_MOCK_DATA=true npm run copy-images:mock && NEXT_USE_MOCK_DATA=true next dev --port 9876",
    "build": "npm run clone && next build && npm run copy-images",
    "clone": "node scripts/clone-docs.mjs",
    "clone:docs": "DOCS_CONTEXT=docs node scripts/clone-docs.mjs",
    "clone:user": "DOCS_CONTEXT=user node scripts/clone-docs.mjs",
    "copy-images": "node scripts/copy-images.mjs",
    "copy-images:mock": "NEXT_USE_MOCK_DATA=true node scripts/copy-images.mjs",
    "test": "jest --maxWorkers=2",
    "test:watch": "jest --maxWorkers=2 --watch",
    "test:alerts": "NODE_OPTIONS=--experimental-vm-modules RUN_ALERTS_TESTS=true jest --maxWorkers=2 alerts.test.ts",
    "test:all": "npm run test && npm run test:alerts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creative-commoners/doc.silverstripe.org.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/creative-commoners/doc.silverstripe.org/issues"
  },
  "homepage": "https://github.com/creative-commoners/doc.silverstripe.org#readme",
  "dependencies": {
    "@docsearch/react": "^4.3.2",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "gray-matter": "^4.0.3",
    "hastscript": "^9.0.1",
    "next": "^16.0.3",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-github-blockquote-alert": "^2.0.0",
    "remark-html": "^16.0.1",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.15.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "autoprefixer": "^10.4.22",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2"
  }
}
